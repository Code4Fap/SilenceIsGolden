{
  "manifest_version": 2,
  "name": "Silence is Golden for FB",
  "short_name": "Silence is Golden for FB",
  "version": "0.0.5",
  "description": "Silence is Golden - Make you invisible on Facebook",
  "homepage_url": "https://ext.huynq.net/",
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "page_action": {
     "default_icon": "img/icon64.png",
     "default_popup": "options.html",
     "default_title": "Silence is Golden - Make you invisible on Facebook"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "img/icon32.png",
    "48": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
